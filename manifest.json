{
  "manifest_version": 3,
  "name": "QuickPath",
  "description": "Save and replay favorite URL paths across tabs",
  "version": "25.1",
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
      "19": "icons/icon19.png",
	  "38": "icons/icon38.png",
	  "48": "icons/icon48.png",
	  "96": "icons/icon96.png",
      "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
	  "38": "icons/icon38.png",
	  "48": "icons/icon48.png",
	  "96": "icons/icon96.png",
      "128": "icons/icon128.png"
    }
  }
}
